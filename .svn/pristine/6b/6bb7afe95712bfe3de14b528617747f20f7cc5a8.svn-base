package net.merise.platform.dao.estate.pojo;

/**
 * 公共设施
 * 
 * @author Administrator
 * 
 */
public class PublicFacilities {

	private String public_facilities_id;
	private String building_id;
	private String house_dong;
	private String facilities_name;
	private String facilities_count;
	private String facilities_status;
	private String facilities_location;
	private String facilities_type;
	private String facilities_linkman;
	private String facilities_remark;
	
	private String facilities_status_text;

	public String getPublic_facilities_id() {
		return public_facilities_id;
	}

	public void setPublic_facilities_id(String public_facilities_id) {
		this.public_facilities_id = public_facilities_id;
	}

	public String getBuilding_id() {
		return building_id;
	}

	public void setBuilding_id(String building_id) {
		this.building_id = building_id;
	}

	public String getHouse_dong() {
		return house_dong;
	}

	public void setHouse_dong(String house_dong) {
		this.house_dong = house_dong;
	}

	public String getFacilities_name() {
		return facilities_name;
	}

	public void setFacilities_name(String facilities_name) {
		this.facilities_name = facilities_name;
	}

	public String getFacilities_count() {
		return facilities_count;
	}

	public void setFacilities_count(String facilities_count) {
		this.facilities_count = facilities_count;
	}

	public String getFacilities_status() {
		return facilities_status;
	}

	public void setFacilities_status(String facilities_status) {
		this.facilities_status = facilities_status;
	}

	public String getFacilities_location() {
		return facilities_location;
	}

	public void setFacilities_location(String facilities_location) {
		this.facilities_location = facilities_location;
	}

	public String getFacilities_type() {
		return facilities_type;
	}

	public void setFacilities_type(String facilities_type) {
		this.facilities_type = facilities_type;
	}

	public String getFacilities_linkman() {
		return facilities_linkman;
	}

	public void setFacilities_linkman(String facilities_linkman) {
		this.facilities_linkman = facilities_linkman;
	}

	public String getFacilities_remark() {
		return facilities_remark;
	}

	public void setFacilities_remark(String facilities_remark) {
		this.facilities_remark = facilities_remark;
	}

	public String getFacilities_status_text() {
		return facilities_status_text;
	}

	public void setFacilities_status_text(String facilities_status_text) {
		this.facilities_status_text = facilities_status_text;
	}
	
