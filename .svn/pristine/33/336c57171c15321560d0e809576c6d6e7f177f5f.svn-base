package net.merise.platform.orm.base;

import java.util.Date;

public class SysUser {

	private long id;
	private String name;
	private String password;
	private String mobile;
	private String email;
	private byte actived;
	private String registrationID;
	private Date createDate;
	private Date lastLoginTime;
	private short type;
	private String remark;
	private String hxName;
	private String functionCustom;
	private String nickName;
	private String sign;
	private String token;

	public void setId(long id) {
		this.id = id;
	}
	public long getId() {
		return id;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public String getPassword() {
		return password;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}
	public String getMobile() {
		return mobile;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getEmail() {
		return email;
	}
	public void setActived(byte actived) {
		this.actived = actived;
	}
	public byte getActived() {
		return actived;
	}
	public void setRegistrationID(String registrationID) {
		this.registrationID = registrationID;
	}
	public String getRegistrationID() {
		return registrationID;
	}
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}
	public Date getCreateDate() {
		return createDate;
	}
	public void setLastLoginTime(Date lastLoginTime) {
		this.lastLoginTime = lastLoginTime;
	}
	public Date getLastLoginTime() {
		return lastLoginTime;
	}
	public void setType(short type) {
		this.type = type;
	}
	public short getType() {
		return type;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getRemark() {
		return remark;
	}
	public void setHxName(String hxName) {
		this.hxName = hxName;
	}
	public String getHxName() {
		return hxName;
	}
	public void setFunctionCustom(String functionCustom) {
		this.functionCustom = functionCustom;
	}
	public String getFunctionCustom() {
		return functionCustom;
	}
	public void setNickName(String nickName) {
		this.nickName = nickName;
	}
	public String getNickName() {
		return nickName;
	}
	public void setSign(String sign) {
		this.sign = sign;
	}
	public String getSign() {
		return sign;
	}
	public void setToken(String token) {
		this.token = token;
	}
	public String getToken() {
		return token;
	}
}
