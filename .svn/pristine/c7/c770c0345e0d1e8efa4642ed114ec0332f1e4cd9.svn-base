<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.merise.platform.orm.mapper.UserEmployeesDAO">
    <insert id="insert" parameterType="net.merise.platform.orm.base.TUserEmployees">insert into T_USER_EMPLOYEES (user_id,employees_id) values(#{user_id},#{employees_id})</insert>
    <delete id="deleteByEmployeesId" parameterType="java.lang.String">delete from T_USER_EMPLOYEES where employees_id = #{employees_id}</delete>
    <delete id="deleteByUserId" parameterType="java.lang.String">delete from T_USER_EMPLOYEES where user_id = #{user_id}</delete>
    <select id="findDataByEmployeesId" parameterType="java.lang.String" resultType="net.merise.platform.orm.base.TUserEmployees">select * from T_USER_EMPLOYEES where employees_id = #{employees_id}</select>
    <select id="findDataByUserId" parameterType="java.lang.String" resultType="net.merise.platform.orm.base.TUserEmployees">select * from T_USER_EMPLOYEES where user_id = #{user_id}</select>
</mapper>
