    <insert id="insert" parameterType="net.merise.platform.dao.estate.pojo.TUserExtend">insert into T_USER_EXTEND (extend_id,user_id,client_id,owner_id) values(#{extend_id},#{user_id},#{client_id},#{owner_id})</insert>
