<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.merise.platform.dao.estate.SysUserDAO">
    <insert id="insert" parameterType="net.merise.platform.dao.estate.pojo.SysUser">insert into SYS_USER (name,password,mobile,email,actived,registrationID,createDate,lastLoginTime,type,remark,hxName,functionCustom,nickName,sign,token) values(#{name},#{password},#{mobile},#{email},#{actived},#{registrationID},#{createDate},#{lastLoginTime},#{type},#{remark},#{hxName},#{functionCustom},#{nickName},#{sign},#{token})</insert>
    <update id="update" parameterType="net.merise.platform.dao.estate.pojo.SysUser">update SYS_USER set name = #{name},password = #{password},mobile = #{mobile},email = #{email},actived = #{actived},registrationID = #{registrationID},createDate = #{createDate},lastLoginTime = #{lastLoginTime},type = #{type},remark = #{remark},hxName = #{hxName},functionCustom = #{functionCustom},nickName = #{nickName},sign = #{sign},token = #{token} where id = #{id}</update>
    <delete id="delete" parameterType="java.lang.String">delete from SYS_USER where id = #{id}</delete>
    <select id="findDataById" parameterType="java.lang.String" resultType="net.merise.platform.dao.estate.pojo.SysUser">select * from SYS_USER where id = #{id}</select>
    <select id="findDataByMobile" parameterType="java.lang.String" resultType="net.merise.platform.dao.estate.pojo.SysUser">select * from SYS_USER where mobile = #{mobile}</select>
    <select id="findAllData" resultType="net.merise.platform.dao.estate.pojo.SysUser">select * from SYS_USER</select>
    <select id="findCount" resultType="java.lang.Integer" parameterType="java.util.Map">select count(*) as count from SYS_USER where mobile like concat('%', #{mobile},'%')</select>
    <select id="findDataByPage" parameterType="java.util.Map" resultType="net.merise.platform.dao.estate.pojo.SysUser">select * from SYS_USER where mobile like concat('%', #{mobile},'%') order by id desc limit ${m}, ${n}</select>
</mapper>
